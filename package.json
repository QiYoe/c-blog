{
  "name": "c-blog",
  "version": "1.0.0",
  "description": "vitepress blog",
  "packageManager": "pnpm@7.5.2",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=14.6.0"
  },
  "scripts": {
    "dev": "vitepress dev",
    "build": "vitepress build",
    "serve": "vitepress serve"
  },
  "keywords": [
    "vite",
    "vue3",
    "vitepress",
    "blog"
  ],
  "homepage": "https://github.com/QiYoe/c-blog/master/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QiYoe/c-blog.git"
  },
  "author": "QiYoe",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "chalk": "^5.0.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog-zh": "^0.0.2",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-multimd-table": "^4.2.0",
    "markdown-it-textual-uml": "^0.12.0",
    "shiki": "^0.10.1",
    "vite": "^3.0.7",
    "vitepress": "1.0.0-alpha.4",
    "vue": "^3.2.37"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  }
}
