{
  "name": "c-blog",
  "version": "1.0.1",
  "description": "vitepress blog",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QiYoe/c-blog.git"
  },
  "author": "QiYoe",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/QiYoe/c-blog",
  "keywords": [
    "vite",
    "vue3",
    "vitepress",
    "blog"
  ],
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "vitepress dev docs",
    "build": "vitepress build docs",
    "serve": "vitepress serve docs",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "cz": "cz",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{docs,mock}/**/*.{vue,ts,tsx,md}\" --fix",
    "lint:prettier": "prettier --write  \"docs/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run bootstrap"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog-zh": "^0.0.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-multimd-table": "^4.1.0",
    "markdown-it-textual-uml": "^0.1.8",
    "prettier": "2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.4.3",
    "vitepress": "^0.20.0",
    "vue-eslint-parser": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-zh"
    }
  },
  "dependencies": {
    "string": "^3.3.3",
    "vue": "^3.2.21"
  }
}
